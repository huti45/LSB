<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#f95b62"></solid>
            <stroke
                android:width="0.5dp"
                android:color="#fafafa"></stroke>
            <corners
                android:radius="45dp"></corners>

        </shape>
    </item>
    <item >
        <shape>
            <solid
                android:color="#f90303"></solid>
            <corners
                android:radius="45dp"></corners>
        </shape>
    </item>
</selector>